	border-color: #9faeda;
	padding-left: 5px;
	padding-right: 5px;
	gridline-color: #9faeda;
}
QTableWidget::item:selected{
	background-color: #8DAFD5;
    color: #f8f8f2;
}
QHeaderView::section{
	background-color: #6272a4;
	max-width: 30px;
	border: none;
	border-style: none;
}
QTableWidget::horizontalHeader {	
	background-color: #6283A4;
}
QHeaderView::section:horizontal
{
    border: 1px solid #6283A4;
	background-color: #6283A4;
	padding: 3px;
